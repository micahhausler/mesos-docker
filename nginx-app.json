{
  "id": "nginx",
  "cmd": null,
  "cpus": 0.2,
  "mem": 128,
  "disk": 0,
  "instances": 2,
  "container": {
    "docker": {
      "image": "nginx",
      "network": "BRIDGE",
      "forcePullImage": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp",
          "name": "http"
        }
      ]
    },
    "type": "DOCKER",
    "volumes": []
  },
  "env": {},
  "labels":{
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "192.168.99.100"
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/index.html",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.5,
    "maximumOverCapacity": 1
  }
}
